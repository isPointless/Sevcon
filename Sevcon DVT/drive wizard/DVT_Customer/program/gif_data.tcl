# proc to convert .gifs to the raw data
proc convert_gif_to_data {fname} {
    package require base64
    set fid [open $fname RDONLY]
    fconfigure $fid -translation binary
    set raw_data [read $fid]
    close $fid
    return [base64::encode $raw_data]
}
